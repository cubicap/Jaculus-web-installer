  },
  "devDependencies": {
    "@types/pako": "^2.0.3",
    "buffer": "^6.0.3",
    "parcel": "^2.8.3",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "parcel-resolver-ignore": "^2.1.5",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.4",
    "web-serial-polyfill": "^1.0.15"
